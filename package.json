{
  "name": "fixated",
  "version": "0.0.1",
  "description": "A utility for creating great test data.",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sfurnival/fixated.git"
  },
  "author": "Stephen Furnival <sfurnival@gmail.com>",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/sfurnival/fixated/issues"
  },
  "homepage": "https://github.com/sfurnival/fixated#readme",
  "eslintConfig": {
    "ecmaFeatures": {
      "ecmaVersion": 6,
      "blockBindings": true,
      "forOf": true,
      "jsx": true
    },
    "rules": {
      "semi": [
        2,
        "always"
      ],
      "comma-dangle": [
        2,
        "never"
      ],
      "no-dupe-args": 2,
      "no-empty": 2,
      "no-extra-parens": [
        2,
        "functions"
      ],
      "no-empty-character-class": 2,
      "no-invalid-regexp": 2,
      "no-unreachable": 2,
      "valid-jsdoc": 2,
      "curly": 2,
      "no-inner-declarations": 2,
      "no-cond-assign": 2,
      "array-bracket-spacing": [
        2,
        "never"
      ],
      "block-spacing": [
        2,
        "always"
      ],
      "comma-spacing": [
        2,
        {
          "before": false,
          "after": true
        }
      ],
      "keyword-spacing": 2,
      "no-duplicate-case": 2,
      "no-debugger": 2,
      "no-constant-condition": 2,
      "valid-typeof": 2,
      "no-alert": 2,
      "yoda": 2,
      "no-multiple-empty-lines": [
        2,
        {
          "max": 2,
          "maxBOF": 0,
          "maxEOF": 1
        }
      ],
      "semi-spacing": 2,
      "object-curly-spacing": [
        2,
        "always"
      ],
      "space-infix-ops": 2
    },
    "env": {
      "es6": true,
      "node": true
    }
  },
  "devDependencies": {
    "eslint": "^3.9.0",
    "gulp": "^3.9.1",
    "gulp-eslint": "^3.0.1",
    "gulp-istanbul": "^1.1.1",
    "gulp-mocha": "^3.0.1",
    "gulp-shell": "^0.5.2",
    "gulp-util": "^3.0.7",
    "ink-docstrap": "^1.3.0",
    "istanbul": "^0.4.5",
    "jsdoc": "^3.4.2",
    "mocha": "^3.1.2",
    "should": "^11.1.1"
  }
}
